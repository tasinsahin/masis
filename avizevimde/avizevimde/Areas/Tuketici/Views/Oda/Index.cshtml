            @Html.DisplayNameFor(model => model.resim)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.resimAdi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.odaAlani)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.odaYukseklik)
