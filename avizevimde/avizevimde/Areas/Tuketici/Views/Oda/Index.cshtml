            @Html.DisplayFor(modelItem => item.resim)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.resimAdi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.odaAlani)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.odaYukseklik)
        </td>
