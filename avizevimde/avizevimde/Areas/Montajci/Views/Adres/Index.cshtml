@model IEnumerable<avizevimde.Areas.Montajci.Models.MontajciAdres>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Montajci/Views/Shared/_Layout.cshtml";
}

            <div>
                    <ul class="breadcrumb">
                        <li>
                            @Html.ActionLink("Home", "Index", "MontajciAnasayfa")<span class="divider">/</span>
                        </li>
                        <li>
                            @Html.ActionLink("Adres", "Index", "MontajciAdres")
                        </li>   
                    </ul>
            </div>   


			<div class="row-fluid sortable">
				<div class="box span12">
					<div class="box-header well" data-original-title>
						<h2><i class="icon-th"></i>Montajcı</h2>
						<div class="box-icon">
							<a href="#" class="btn btn-setting btn-round"><i class="icon-cog"></i></a>
							<a href="#" class="btn btn-minimize btn-round"><i class="icon-chevron-up"></i></a>
							<a href="#" class="btn btn-close btn-round"><i class="icon-remove"></i></a>
						</div>
					</div>
					<div class="box-content">
                  	<div class="row-fluid">
                    <p>
                        @Html.ActionLink("Create New", "Create")
                    </p>
                    <table>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.montajciId)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.sehirId)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ilceId)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.semtId)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.mahalleId)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.cadde)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.sokak)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.numara)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.enlem)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.boylam)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.adresAdi)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.adresFatura)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.adresTeslimat)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.EklenmeTarihi)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.degistirilenId)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.enYeni)
                            </th>
                            <th></th>
                        </tr>

                    @foreach (var item in Model) {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.montajciId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.sehirId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ilceId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.semtId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.mahalleId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.cadde)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.sokak)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.numara)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.enlem)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.boylam)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.adresAdi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.adresFatura)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.adresTeslimat)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.EklenmeTarihi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.degistirilenId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.enYeni)
                            </td>
                            <td>
                                @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                                @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                                @Html.ActionLink("Delete", "Delete", new { id=item.Id })
                            </td>
                        </tr>
                    }

                    </table>
                  	</div>                   
                  </div>
				</div><!--/span-->
			</div><!--/row-->

