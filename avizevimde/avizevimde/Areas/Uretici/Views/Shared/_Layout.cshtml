	<script src="~/Areas/Admin/Content/js/jquery.autogrow-textarea.js" type="text/javascript"></script>
	<!-- multiple file upload plugin -->
	<script src="~/Areas/Admin/Content/js/jquery.uploadify-3.1.min.js" type="text/javascript"></script>
	<!-- history.js for cross-browser state change on ajax -->
	<script src="~/Areas/Admin/Content/js/jquery.history.js" type="text/javascript"></script>
	<!-- application script for Charisma demo -->
		<script src="~/Areas/Admin/Content/js/charisma.js" type="text/javascript"></script>
