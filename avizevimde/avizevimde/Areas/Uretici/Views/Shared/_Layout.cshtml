	<script src="~/Areas/Admin/Content/js/jquery.uniform.min.js" type="text/javascript"></script>
	<!-- plugin for gallery image view -->
	<script src="~/Areas/Admin/Content/js/jquery.colorbox.min.js" type="text/javascript"></script>
