@model IEnumerable<avizevimde.Areas.Uretici.Models.uretici>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Uretici/Views/Shared/_Layout.cshtml";
}
            <div>
                    <ul class="breadcrumb">
                        <li>
                            @Html.ActionLink("Home", "Index", "UreticiAnasayfa")<span class="divider">/</span>
                        </li>
                        <li>
                            @Html.ActionLink("Uretici", "Index", "Uretici") 
                    </ul>
            </div> 


			<div class="row-fluid sortable">
				<div class="box span12">
					<div class="box-header well" data-original-title>
						<h2><i class="icon-th"></i>Üretici</h2>
						<div class="box-icon">
							<a href="#" class="btn btn-setting btn-round"><i class="icon-cog"></i></a>
							<a href="#" class="btn btn-minimize btn-round"><i class="icon-chevron-up"></i></a>
							<a href="#" class="btn btn-close btn-round"><i class="icon-remove"></i></a>
						</div>
					</div>
					<div class="box-content">
                  	<div class="row-fluid">

                    <p>
                        @Html.ActionLink("Create New", "Create")
                    </p>
                    <table>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.adi)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ureticiAdi)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ureticiTanim)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ureticiTelefon1)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ureticiTelefon2)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ureticiCepTelefonu)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ureticiEpostaAdresi)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ozelTasarimYapiyor)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.eklenmeTarihi)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.degistirmeTarihi)
                            </th>
                            <th></th>
                        </tr>

                    @foreach (var item in Model) {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.adi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ureticiAdi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ureticiTanim)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ureticiTelefon1)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ureticiTelefon2)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ureticiCepTelefonu)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ureticiEpostaAdresi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ozelTasarimYapiyor)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.eklenmeTarihi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.degistirmeTarihi)
                            </td>
                            <td>
                                @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
                                @Html.ActionLink("Details", "Details", new { id=item.id }) |
                                @Html.ActionLink("Delete", "Delete", new { id=item.id })
                            </td>
                        </tr>
                    }

                    </table>
                    </div>                   
                  </div>
				</div><!--/span-->
			</div><!--/row-->

