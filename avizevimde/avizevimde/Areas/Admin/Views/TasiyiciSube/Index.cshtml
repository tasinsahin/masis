@model IEnumerable<avizevimde.Areas.Admin.Models.tasiyiciSube>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";


                                <div>
                    <ul class="breadcrumb">
                        <li>
                            @Html.ActionLink("Home", "Index", "AdminAnasayfa")<span class="divider">/</span>
                        </li>
                        <li>
                            @Html.ActionLink("Taşıyıcı Şube", "Index", "TasiyiciSube")
                    </ul>
            </div>
    
    
			<div class="row-fluid sortable">
				<div class="box span12">
					<div class="box-header well" data-original-title>
						<h2><i class="icon-th"></i>Taşıyıcı Şube</h2>
						<div class="box-icon">
							<a href="#" class="btn btn-setting btn-round"><i class="icon-cog"></i></a>
							<a href="#" class="btn btn-minimize btn-round"><i class="icon-chevron-up"></i></a>
							<a href="#" class="btn btn-close btn-round"><i class="icon-remove"></i></a>
						</div>
					</div>
					<div class="box-content">
                  	<div class="row-fluid">
                    <p>
                        @Html.ActionLink("Create New", "Create")
                    </p>
                    <table>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.subeAdi)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.sehir.sehirAdi)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ilce.ilceAdi)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.semt.semtAdi)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.mahalle.mahalleAdi)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.cadde)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.sokak)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.numara)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.faturaAdresi)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.enlem)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.boylam)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.EklenmeTarihi)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ekleyen.adminAdi)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.DegistirmeTarihi)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.degistiren.adminAdi)
                            </th>
                            <th></th>
                        </tr>

                    @foreach (var item in Model) {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.subeAdi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.sehir.sehirAdi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ilce.ilceAdi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.semt.semtAdı)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.mahalle.mahalleAdi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.cadde)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.sokak)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.numara)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.faturaAdresi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.enlem)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.boylam)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.EklenmeTarihi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ekleyen.adminAdi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.DegistirmeTarihi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.degistiren.adminAdi)
                            </td>
                            <td>
                                @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                                @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                                @Html.ActionLink("Delete", "Delete", new { id=item.Id })
                            </td>
                        </tr>
                    }

                    </table>
                  	</div>                   
                  </div>
				</div><!--/span-->
			</div><!--/row-->

