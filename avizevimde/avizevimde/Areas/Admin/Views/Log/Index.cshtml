@model IEnumerable<avizevimde.Areas.Admin.Models.adminLog>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

                              <div>
                    <ul class="breadcrumb">
                        <li>
                            @Html.ActionLink("Home", "Index", "AdminAnasayfa")<span class="divider">/</span>
                        </li>
                        <li>
                            @Html.ActionLink("Log", "Index", "Log")
                    </ul>
            </div>


			<div class="row-fluid sortable">
				<div class="box span12">
					<div class="box-header well" data-original-title>
						<h2><i class="icon-th"></i>Log</h2>
						<div class="box-icon">
							<a href="#" class="btn btn-setting btn-round"><i class="icon-cog"></i></a>
							<a href="#" class="btn btn-minimize btn-round"><i class="icon-chevron-up"></i></a>
							<a href="#" class="btn btn-close btn-round"><i class="icon-remove"></i></a>
						</div>
					</div>
					<div class="box-content">
                  	<div class="row-fluid">
                    <table>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.logKaynak)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.logOlay)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.admin.adminAdi)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.eklenmeTarihi)
                            </th>
                            <th></th>
                        </tr>

                    @foreach (var item in Model) {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.logKaynak)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.logOlay)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.admin.adminAdi)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.eklenmeTarihi)
                            </td>
                            <td>
                                @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                                @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                                @Html.ActionLink("Delete", "Delete", new { id=item.Id })
                            </td>
                        </tr>
                    }

                    </table>
                  	</div>                   
                  </div>
				</div><!--/span-->
			</div><!--/row-->

